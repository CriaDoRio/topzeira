//fatorial
#include <stdio.h>
#include <stdlib.h>

unsigned long long fatorial(int n) {
    unsigned long long resultado = 1;

    for (int i = 2; i <= n; i++) {
        resultado *= i;
    }

    return resultado;
}
int main (){
    int nu;
    scanf("%d", &nu);
    printf("%d eh %llu\n", nu, fatorial(nu));
}
