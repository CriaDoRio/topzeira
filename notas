//notas prova 1
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int main() {
    float n;
    scanf("%f", &n);

    if (n < 0 || n > 10) {
        return 0;
    } else {
        if (n < 3) {
            printf("nota E\n");
        } else if (n < 6) {
            printf("nota D\n");
        } else if (n < 7.5) {
            printf("nota C\n");
        } else if (n < 9) {
            printf("nota B\n");
        } else {
            printf("nota A\n");
        }
    }

    return 0;
}
}
