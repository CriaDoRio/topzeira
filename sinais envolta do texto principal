//coloca asteriscos na volta
#include <stdio.h>
#include <locale.h>

imprimeLinha() {
    int i, qtd = 20;
    printf ("\n");
    for(i = 1; i <=qtd; i++){
        printf("*");
    }
    printf("\n");
}
main (){
setlocale (LC_ALL, "Portuguese");
imprimeLinha (10, 20);
printf ("textos :)!");
imprimeLinha (10, 20);
}
